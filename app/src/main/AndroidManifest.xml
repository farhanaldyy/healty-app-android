<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.healtyapp">

    <uses-permission android:name="android.permission.INTERNET"/>
    
    <application
        android:allowBackup="true"
        android:icon="@mipmap/icons8_hospital_90px"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icons8_hospital_90px"
        android:supportsRtl="true"
        android:theme="@style/Theme.HealtyApp"
        android:usesCleartextTraffic="true">
        <activity android:name=".activity.RegisterActivity" android:theme="@style/Theme.AppCompat.NoActionBar">
        </activity>
        <activity
            android:name=".MainActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.LoginActivity" android:theme="@style/Theme.AppCompat.NoActionBar"/>
        <activity android:name=".activity.MasukActivity" android:theme="@style/Theme.AppCompat.NoActionBar"/>
    </application>

</manifest>